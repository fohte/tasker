    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "drizzle-orm": "0.43.1",
    "graphql": "16.11.0",
    "graphql-request": "7.1.2",
    "graphql-yoga": "5.13.5",
    "lucide-react": "0.511.0",
    "next": "15.3.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "swr": "2.3.3",
    "tailwind-merge": "3.3.1",
    "uuid": "11.1.0"
